  @puzzle Advent.Puzzle.load(<%=@year%>, <%=@day%>)

  test "run part1 puzzle" do
    assert Part1.run(@puzzle) == 42
  end

  @tag :skip
  test "run part2 puzzle" do
    assert Part2.run(@puzzle) == 42
  end
end
